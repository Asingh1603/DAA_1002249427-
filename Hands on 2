# Implement:
# 1. insertion sort

def insertion_sort (HO2_1)
      for i in range(1, len (HO2_1)):
            for j in range (i-1, 0, -1):
                if HO2_1[j] > HO2_1[j +1]:
                    HO2_1[j], HO2_1[j +1] = HO2_1[j +1], HO2_1[j]
                else:
                    break


